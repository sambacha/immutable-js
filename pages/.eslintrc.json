{
  "env": {
    "browser": true
  },
  "parser": "@babel/eslint-parser",
  "extends": "../.eslintrc.json",
  "rules": {
    "react/static-property-placement": "off",
    "react/forbid-prop-types": "off",
    "react/require-default-props": "off",
    "camelcase": ["error", { "allow": ["^UNSAFE_"] }]
  }
}
